stuff "git status^M"

screen -t se:new bash
stuff "cd ~/g/git^M"
stuff "# gpp -F --fpa='--subject-prefix=\"PATCH\"' --sea='--cc=\"Junio C Hamano <gitster@pobox.com>\"' --no-send^M"
