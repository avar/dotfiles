stuff "# time NO_SVN_TESTS=1 GIT_TEST_HTTPD=0 make GIT_PROVE_OPTS=\"--jobs=\$(nproc) --timer --state=failed,slow,save\"^M"
stuff "# time GIT_TEST_HTTPD=1 make GIT_PROVE_OPTS=\"--jobs=\$(nproc) --timer --state=failed,slow,save\"^M"

screen -t t:1 bash
stuff "cd ~/g/git^M"
stuff "cd t^M"
stuff "time make GIT_PROVE_OPTS=\"--jobs=\$(nproc) --timer\" T='t[0-9]*grep*.sh t[0-9]*log*.sh'"

