stuff "cd ~/g/git^M"
stuff "remove_from_path \$PWD/bin-wrappers^M"
stuff "cd t^M"
stuff "# ~/g/git.meta/config.mak.sh && time make GIT_PROVE_OPTS=\"--timer --state=save\" T=\"$(dedup $(echo t[0-9]*{grep,log}*.sh))\"^M"
stuff "# ~/g/git.meta/config.mak.sh && time make GIT_PROVE_OPTS=\"--timer --state=save\" T=\"$(dedup $(echo t[0-9]*{bundle,protocol,serve,pack,remote,receive}*.sh))\""
stuff "# ~/g/git.meta/config.mak.sh && time make GIT_PROVE_OPTS=\"--timer --state=save\" T=\"$(dedup $(git grep -l -e submodule t[0-9]*.sh))\""
stuff "# ~/g/git.meta/config.mak.sh && time make GIT_TEST_PASSING_SANITIZE_LEAK=check GIT_TEST_SANITIZE_LEAK_LOG=true GIT_PROVE_OPTS=\"--timer --state=save\" T=\"$(dedup $(git grep -l -e submodule t[0-9]*.sh))\" GIT_TEST_OPTS_EXTRA=--immediate"

