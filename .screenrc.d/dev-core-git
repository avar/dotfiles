stuff "# git rixr -x 'make git-objs'^M"
stuff "# git rixr -x 'GIT_TEST_PASSING_SANITIZE_LEAK=check GIT_TEST_SANITIZE_LEAK_LOG=true GIT_SKIP_TESTS='t2012 t6435 t7517' make SANITIZE=leak test^M"

